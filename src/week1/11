import random
secret_number = random.randint(1, 5)
guess = None
print(secret_number)


while guess != secret_number:

    # 获取用户输入
    guess = int(input("请输入你猜的数字（1到5之间）："))
    
    if guess < secret_number:
        print("你猜的数字太小了！")
    elif guess > secret_number:
        print("你猜的数字太大了！")
    else:
        print("恭喜你，猜对了！")
1
